}
 /*Header starts*/

 header {
      position: fixed;
      top: 0;
      left: 0;
      width: 100%;
      z-index: 100;
    }

    .container {
      max-width: 960px;
      margin: auto;
      width: 100%;
    }

    .header-active {
      /* background-color: #393a61; */
      transition: 0.4s;
      height: 80px;
    }

    .nav {
      display: flex;
      justify-content: space-between;
      align-items: center;
      padding: 20px 0;
    }

    .logo {
      font-size: 2rem;
      font-weight: 800;
      position: relative;
      top: -10px;
      text-decoration: none;
      color: #6cebd5;
    }

    .navbar {
      display: flex;
      list-style: none;
      
      align-items: center;
      column-gap: 1.5rem;
    }

    .nav-link {
      font-weight: bold;
      /* color: rgb(63, 2, 63); */
      color: #393a61;
      font-size: 1.1rem;
      
      padding: 4px 8px;
      border-radius: 0.2rem;
    }

    .nav-link:hover {
      background-color: #393a61;
      border: 1px solid #393a61;
      color: white;
      /* color: white;
        background: rgb(63, 2, 63); */
      transition: 0.3s all linear;
    }

  
    /*Header ends*/
    /* menu icon starts */

    .menu-icon {
      display: none;
      flex-direction: column;
      align-items: center;
      justify-content: center;
      row-gap: 5px;
      cursor: pointer;
      z-index: 200;
      transition: 0.3s;
    }

    .menu-icon div {
      display: block;
      background: #393a61;
      height: 2px;
      width: 24px;
      transition: 0.3s;
    }

    .move .line1 {
      transform: rotate(-45deg) translate(-5px, 5px);
    }

    .move .line2 {
      opacity: 0;
    }

    .move .line3 {
      transform: rotate(45deg) translate(-5px, -5px);
    }

    /* menu-icon ends */
    @media (max-width: 892px) {
      section {
        padding: 2rem 0;
      }

      .nav {
        padding: 12px 0;
      }

      .menu-icon {
        display: flex;
      }

      .navbar {
        position: absolute;
        top: 0;
        right: -100%;
        width: 300px;
        height: 70vh;
        background: #fff;
        display: flex;
        flex-direction: column;
        align-items: center;
        padding: 50px 20px;
        transition: 0.5s all cubic-bezier(0.075, 0.82, 0.165, 1);
      }

      .open-menu {
        right: 0;
        transition: 0.5s all cubic-bezier(0.075, 0.82, 0.165, 1);
      }
    }

    @media (max-width: 775px) {
      main {
        height: -webkit-fill-available;
      }

      .logo {
        font-size: 1.1rem;
        top: 0px;
        margin: 10px;
      }

      .menu-icon {
        display: flex;
        margin-right: 10px;
      }

      .navbar {
        position: absolute;
        top: 0;
        right: -100%;
        width: 300px;
        height: 70vh;
        background: #6cebd5;
        display: flex;
        flex-direction: column;
        align-items: center;
        padding: 50px 20px;
        transition: 0.5s all cubic-bezier(0.075, 0.82, 0.165, 1);
      }

      .open-menu {
        right: 0;
        transition: 0.5s all cubic-bezier(0.075, 0.82, 0.165, 1);
      }

      .nav-link {
        display: block;
        margin: 1rem 0;
      }
    }

